<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.*?>
<?import java.lang.*?>

<BorderPane fx:id="root" xmlns="http://javafx.com/javafx/8.0.111"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.kuberam.getos.modules.pdfEditor.PdfEditorController">
	<top>
		<ToolBar prefHeight="40.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<items>
				<ComboBox fx:id="selectEditorCombobox" minWidth="70.0"
					prefWidth="90.0">
					<items>
						<FXCollections fx:factory="observableArrayList">
							<String fx:value="jpedal" />
							<String fx:value="pdfbox" />
						</FXCollections>
					</items>
					<tooltip>
						<Tooltip text="%select_viewer_tooltip" />
					</tooltip>
				</ComboBox>
				<TextField fx:id="currentPageTextfield" prefHeight="26.0"
					prefWidth="46.0"
					style="-fx-border-radius: 5 0 0 5; -fx-background-radius: 5 0 0 5;" />
				<Label fx:id="pgCountLabel"
					style="-fx-border-radius: 0 5 5 0; -fx-background-radius: 0 5 5 0;" />
				<ComboBox editable="true" value="$defaultSelection"
					style="-fx-border-radius: 5 0 0 5; -fx-background-radius: 5 0 0 5;"
					minWidth="80.0" prefWidth="90.0">
					<items>
						<FXCollections fx:factory="observableArrayList">
							<String fx:value="NVT" />
							<String fx:value="Bezig" />
							<String fx:value="Positief" />
							<String fx:value="Negatief" />
						</FXCollections>
					</items>
				</ComboBox>
				<Pane HBox.hgrow="ALWAYS" />
				<Button fx:id="extractTablesButton" mnemonicParsing="false"
					text="Extract Tables" />
			</items>
		</ToolBar>
	</top>
	<center>
		<ScrollPane fx:id="centerSourcePane" pannable="true"
			BorderPane.alignment="CENTER">
			<content>
				<Group fx:id="contentSourcePane" />
			</content>
		</ScrollPane>
	</center>
</BorderPane>
